{
  "name": "fetch-graph",
  "version": "0.0.0",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "test": "jest",
    "test:unit": "npm test \"src/**/!(index).test.js\"",
    "test:integration": "npm test \"src/**/index.test.js\"",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "opn docs/index.html"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "apidoc-markdown": "^0.2.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.5",
    "dotenv-safe": "^4.0.3",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest-cli": "^21.1.0",
    "mockgoose": "^7.3.3",
    "nock": "^9.0.2",
    "nodemon": "^1.10.2",
    "opn-cli": "^3.1.0",
    "sinon": "^3.3.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.0"
  },
  "dependencies": {
    "@fetchdrones/contact": "^1.1.8",
    "@fetchdrones/messages": "^1.3.0",
    "@fetchdrones/order": "^1.1.3",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.1.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "bodymen": "^1.0.1",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "docker-names": "^1.0.3",
    "elasticsearch": "^13.3.1",
    "express": "^4.13.4",
    "jsonwebtoken": "^8.0.1",
    "kafka-node": "^2.2.3",
    "kafka-streams": "^3.0.0",
    "lodash": "^4.13.1",
    "mongoosastic": "^4.3.0",
    "mongoose": "^4.11.12",
    "mongoose-create-unique": "^0.4.1",
    "mongoose-events": "^0.3.9",
    "mongoose-keywords": "^0.3.2",
    "mongoose-trigger": "^1.0.1",
    "morgan": "^1.7.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^3.0.0",
    "pm2": "^2.4.5",
    "querymen": "^2.0.0",
    "rand-token": "^0.3.0",
    "redis": "^2.7.1",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "throng": "^4.0.0",
